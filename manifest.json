{
    "manifest_version": 2,
    "name": "4K Extension",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": ["https://www.google.com/search?*"],
          "css": ["google_search.css"],
          "document_end" : "document_idle"
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    }
}